<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".main.view.MainActivity">

    <FrameLayout
        android:id="@+id/main_framelayout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="15"></FrameLayout>

    <com.flyco.tablayout.CommonTabLayout
        android:id="@+id/main_common"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:tl_textSelectColor="@color/maintextSelectColor"
        app:tl_textUnselectColor="@color/maintextUnselectColor"
        app:tl_iconHeight="@dimen/icon30"
        app:tl_iconWidth="@dimen/icon30"
        android:layout_weight="1"></com.flyco.tablayout.CommonTabLayout>


</LinearLayout>